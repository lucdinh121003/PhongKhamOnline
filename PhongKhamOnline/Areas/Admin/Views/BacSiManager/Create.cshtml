@model PhongKhamOnline.Models.BacSi

@{
    ViewData["Title"] = "Create";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Danh sách lịch hẹn</title>
    <link rel="stylesheet" href="~/css/createBS.css">
</head>

<body>
    <div class="container">
        <header>Form Header</header>
        <div class="form">
            <form asp-action="Create" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="input-box">
                    <label asp-for="Ten" class="control-label"></label>
                    <input asp-for="Ten" class="form-control" />
                    <span asp-validation-for="Ten" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="SoDienThoai" class="control-label"></label>
                    <input asp-for="SoDienThoai" class="form-control" />
                    <span asp-validation-for="SoDienThoai" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="ChuyenMonBacSiId" class="control-label"></label>
                    <div class="select-box">
                        <select asp-for="ChuyenMonBacSiId" asp-items="ViewBag.ChuyenMonBacSi" class="form-control"></select>
                    </div>
                </div>

                <div class="input-box">
                    <label asp-for="DiaChiPhongKham" class="control-label"></label>
                    <input asp-for="DiaChiPhongKham" class="form-control" />
                    <span asp-validation-for="DiaChiPhongKham" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="DiaChiThuongTru" class="control-label"></label>
                    <input asp-for="DiaChiThuongTru" class="form-control" />
                    <span asp-validation-for="DiaChiThuongTru" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="Email" class="control-label"></label>
                    <input asp-for="Email" class="form-control" />
                    <span asp-validation-for="Email" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="SoDienThoaiPhongKham" class="control-label"></label>
                    <input asp-for="SoDienThoaiPhongKham" class="form-control" />
                    <span asp-validation-for="SoDienThoaiPhongKham" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="MoTa" class="control-label"></label>
                    <input asp-for="MoTa" class="form-control" />
                    <span asp-validation-for="MoTa" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="SoHenToiDaTrongNgay" class="control-label"></label>
                    <input asp-for="SoHenToiDaTrongNgay" class="form-control" />
                    <span asp-validation-for="SoHenToiDaTrongNgay" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="AnhDaiDien" class="control-label"></label>
                    <input type="file" asp-for="AnhDaiDien" class="form-control" />
                </div>

                <div class="input-box">
                    <label asp-for="SoTaiKhoan" class="control-label"></label>
                    <input asp-for="SoTaiKhoan" class="form-control" />
                    <span asp-validation-for="SoTaiKhoan" class="text-danger"></span>
                </div>

                <div class="input-box">
                    <label asp-for="KhungGioBacSiId" class="control-label"></label>
                    <div class="select-box">
                        <select asp-for="KhungGioBacSiId" asp-items="ViewBag.KhungGioBacSis" class="form-control"></select>
                    </div>
                </div>

                <div class="input-box">
                    <button type="submit">Create</button>
                </div>
            </form>
        </div>
    </div>


    <div>
        <a asp-action="Index">Back to List</a>
    </div>
    @section Scripts {
        @{
            await Html.RenderPartialAsync("_ValidationScriptsPartial");
        }
    }

</body>


